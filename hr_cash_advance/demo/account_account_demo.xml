<odoo>
<record id="employe_advance_account" model="account.account">
    <field name="name">Employee Advance</field>
    <field name="user_type_id" ref="account.data_account_type_current_assets"/>
    <field name="code">EA01</field>
    <field name="reconcile" eval="True"/>
</record>

<record id="employe_advance_payable_account" model="account.account">
    <field name="name">Employee Advance Payable</field>
    <field name="user_type_id" ref="account.data_account_type_current_liabilities"/>
    <field name="code">EA02</field>
    <field name="reconcile" eval="True"/>
</record>
</odoo>
